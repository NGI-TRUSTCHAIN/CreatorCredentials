{
  "name": "creator-credentials-ui",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "^16",
    "pnpm": "8.8.0"
  },
  "packageManager": "pnpm@8.8.0",
  "scripts": {
    "build": "next build",
    "changeset:build": "pnpm tsc ./.changeset/changelog.generator.ts",
    "check:licenses": "node check-licenses.js",
    "docker:base:dev": "docker compose --env-file .env.local -f docker/docker-compose.local.yaml up -d --build",
    "docker:base:prod": "docker compose --env-file .env.local.prod -f docker/docker-compose.prod.yaml up -d --build",
    "lint": "next lint --max-warnings=0",
    "prepare": "husky install",
    "prettier:check": "prettier -c .",
    "prettier:fix": "prettier . --write",
    "pre-commit": "lint-staged",
    "pre-push": "pnpm prettier:check && pnpm lint && pnpm test:unit --run && pnpm changeset status",
    "start:dev": "next dev -p $FRONTEND_APP_PORT",
    "start:docker": "pnpm docker:base:dev up -V",
    "start:docker:prod": "pnpm docker:base:prod up",
    "start:local": "next dev",
    "start:prod": "next start -p $FRONTEND_APP_PORT",
    "start:prod:local": "next start",
    "test:unit": "tsc --p tsconfig.test.json && vitest"
  },
  "dependencies": {
    "@clerk/nextjs": "4.29.3",
    "@hookform/resolvers": "3.3.2",
    "@metamask/sdk": "0.12.0",
    "@tanstack/react-query": "4.36.1",
    "axios": "1.5.1",
    "clsx": "2.0.0",
    "flowbite": "1.8.1",
    "flowbite-react": "0.6.4",
    "i18next": "23.5.1",
    "i18next-http-backend": "2.2.2",
    "joi": "17.11.0",
    "msw": "1.3.2",
    "next": "13.5.5",
    "next-auth": "4.24.3",
    "next-i18next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.47.0",
    "react-hot-toast": "2.4.1",
    "react-i18next": "13.3.0",
    "tailwind-merge": "1.14.0"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.2",
    "@changesets/types": "5.2.1",
    "@svgr/webpack": "8.1.0",
    "@tanstack/react-query-devtools": "4.36.1",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/jest": "29.5.5",
    "@types/node": "20.8.6",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "@vitejs/plugin-react": "4.1.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.51.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-no-only-tests": "3.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "license-checker-rseidelsohn": "4.2.10",
    "lint-staged": "15.0.1",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "vite-plugin-svgr": "4.1.0",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  }
}
